# Server
PORT=8080

# Default upstream target base (optional if using header/query/path override)
TARGET_URL=https://httpbin.org

# CORS
ALLOWED_ORIGINS=*
CORS_ALLOW_CREDENTIALS=false
CORS_MAX_AGE=600

# Proxy behavior
ALLOW_TARGET_HEADER=true
ALLOW_TARGET_QUERY=true
# Allow embedding full target URL in the path: /https://example.com/foo
PATH_TARGET_MODE=true
FORWARD_PATH=true
STRIP_PREFIX=
SECURE_PROXY=true
PROXY_TIMEOUT_MS=30000

# Public links
LINKEDIN_URL=
GITHUB_URL=

# Blocklist response Content-Types (comma-separated). Supports wildcards like text/*
DISABLE_CONTENT_TYPES=text/html
